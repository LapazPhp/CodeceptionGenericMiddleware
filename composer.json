{
    "name": "lapaz/codeception-generic-middleware",
    "description": "Generic Middleware Functional Test Module for Codeception",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Hisateru Tanaka",
            "email": "tanakahisateru@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.1",
        "codeception/codeception": "^2.2||^2.1.7",
        "http-interop/http-factory-diactoros": "^1.0",
        "psr/container": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-server-middleware": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3"
    },
    "suggest": {
        "http-interop/http-factory-slim": "Slim version of PSR-17 implementations"
    },
    "autoload": {
        "psr-4": {
            "Lapaz\\Codeception\\Psr15\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lapaz\\Codeception\\Psr15\\": "tests"
        }
    }
}
